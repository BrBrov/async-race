{
    "parser": "@typescript-eslint/parser",
    "plugins": ["prettier", "import", "@typescript-eslint"],
    "extends": [
        "airbnb",
        "airbnb-typescript/base"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off",
        "@typescript-eslint/no-explicit-any": "error"
    }
}
